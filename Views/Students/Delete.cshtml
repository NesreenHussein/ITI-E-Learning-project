@model Student
<h1 class="alert alert-danger">Are you sure you want to delete this?</h1>


<partial name="_StudentsData" />

<form method="post" asp-controller="Students" asp-action="DeleteCurrent">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Delete</button>

</form>
<a asp-controller="Students" asp-action="GetIndexView">Back To List</a>






